{% block_start car %}

<Car>
components:
    <IdentityComponent>
        <property name="id" value="cat" />
    </IdentityComponent>
    <CatComponent>
        <property name="type" value=v"0" />
        <property name="color" value=v"0xFF0000" />
        <property name="max_speed" value=v"100" />
        <property name="acceleration" value=v"0.4" />
    </CatComponent>
</Car>

{% block_finish car %}

<body>
childs:
    {% block car %}
    {% block car %}
</body>
